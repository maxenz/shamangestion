

<%= simple_form_for([@cliente,@clientes_contacto], :html => {:class => 'formPrincipal'}) do |f| %>

  <div class="form-inputs">
    <%= f.input :nombre %>
    <%= f.input :email %>
    <%= f.input :flgPrincipal %>
    <%= f.input :telefono %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Guardar', :class => 'btn btn-primary' %>
  </div>
<% end %>


